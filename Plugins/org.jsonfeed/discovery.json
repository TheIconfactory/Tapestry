{
	"html": [
		{
			"element": "link",
			"check": "type",
			"match": "/application/(?:json|feed\\+json)\\s*$/",
			"use": "href",
			"extract": "/^((?!wp-json).)*$/",
			"comment": "The regex above lets this rule filter out WordPress API links",
		},
		{
			"element": "link",
			"check": "type",
			"match": "/application/(?:json|feed\\+json)\\s*$/",
			"use": "href",
			"variable": "site"
		}
	],
	"json": [
		{
			"key": "version",
			"value": "https://jsonfeed.org/version/1"
		},
		{
			"key": "version",
			"value": "https://jsonfeed.org/version/1.1"
		}
	]
}
